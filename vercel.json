{
  "rewrites": [
    {
      "source": "/",
      "destination": "/index.html"
    },
    {
      "source": "/en",
      "destination": "/en/index.html"
    },
    {
      "source": "/ar",
      "destination": "/ar/index.html"
    },
    {
      "source": "/en/(.*)",
      "destination": "/en/$1.html"
    },
    {
      "source": "/ar/(.*)",
      "destination": "/ar/$1.html"
    },
    {
      "source": "/(.*)",
      "destination": "/$1.html"
    }
  ],
  "trailingSlash": false
}
